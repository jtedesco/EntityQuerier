set terminal png enhanced font verdana 15 size 1400,1000
set output '%s'
set title '%s'

set style data histogram
set style histogram cluster gap 3

set xlabel "%s"

set style fill solid border rgb "black"
set auto x
set yrange [0:*]
plot 'input.dat' using 2:xtic(1) title col, \
        'input.dat' using 3:xtic(1) title col, \
        'input.dat' using 4:xtic(1) title col, \
        'input.dat' using 5:xtic(1) title col, \
        'input.dat' using 6:xtic(1) title col, \
        'input.dat' using 7:xtic(1) title col, \
        'input.dat' using 8:xtic(1) title col, \
        'input.dat' using 9:xtic(1) title col, \
        'input.dat' using 10:xtic(1) title col, \
        'input.dat' using 11:xtic(1) title col